%%Muestreo de un coseno

%%Se単al continua
t = 0:0.1:20;
s = cos(t);

%%Se単al muestreada con 50 muestras
X = linspace(0,6*pi,50)';
Y = (cos(X));



figure(1);subplot(2,1,1);plot(t,s); grid;title('Se単al cos(t)');xlabel('tiempo (segundos)');ylabel ('magnitud');
figure(1);subplot(2,1,2);stem(Y);grid;title('Se単al cos(t) muestreada');
